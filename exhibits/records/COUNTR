COUNTR
      SUBROUTINE COUNTR(JPT,JND,J)
      DIMENSION JPT(100)
C 
C  FOLLOW JPT CHAIN STARTING AT JND AND 
C  RETURN COUNT OF NODES IN J.
C 
*CALL,DATA
C 
      IND=JND 
      J=0 
      DO 102 I=1,MDA
      IF(IND.EQ.0) RETURN 
      IND=JPT(IND)
102   J=J+1 
      CALL ERROR(1) 
      RETURN
      END 
