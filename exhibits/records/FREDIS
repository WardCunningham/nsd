FREDIS
      SUBROUTINE FREDIS(JF,JN)
C 
C  CLEAR DISPLAY SEGMENTS FOR SUBTREE HEADED BY JF. 
C  IF JN IS NON-ZERO, IT AND ITS SON SUBTREE WILL NOT 
C  BE CLEARED.
C 
*CALL,DATA
C 
      CALL PUSH(0)
      CALL PUSHNZ(JF) 
102   CALL PULL(IN) 
      IF(IN.EQ.0) RETURN
      CALL PUSHNZ(LBT(IN))
      CALL PUSHNZ(LNX(IN))
      IF(IN.EQ.JN) GOTO 102 
      IF(LSG(IN).NE.0) CALL FREE(LSG(IN)) 
      CALL PUSHNZ(LSN(IN))
      GOTO 102
C 
      END 
