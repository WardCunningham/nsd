FRETRE
      SUBROUTINE FRETRE(JN) 
C 
C  DEALLOCATE NODE JN AND ALL NODES IN THE SUBTREE FOR WHICH IT 
C  IS THE ROOT.  JN IS SET TO NULL. 
C 
*CALL,DATA
C 
      IN=JN 
      IF(IN.EQ.0) RETURN
      CALL PUSH(0)
102   CALL PUSHNZ(LNX(IN))
      CALL PUSHNZ(LBT(IN))
      CALL PUSHNZ(LSN(IN))
      CALL FRENOD(IN) 
      CALL PULL(IN) 
      IF(IN.NE.0) GOTO 102
      JN=0
      RETURN
      END 
