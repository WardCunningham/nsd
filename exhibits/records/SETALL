SETALL
      SUBROUTINE SETALL(JPT,JND,JAM,JAX,JM,JX)
      DIMENSION JPT(100),JAM(100),JAX(100)
C 
C  FOLLOW JPT CHAIN STARTING AT JND AND 
C  SET ALL JAX AND JAM TO JX AND JM RESPECTIVLY.
C 
*CALL,DATA
C 
      IND=JND 
      DO 102 I=1,MDA
      IF(IND.EQ.0) RETURN 
      JAX(IND)=JX 
      JAM(IND)=JM 
102   IND=JPT(IND)
      CALL ERROR(2) 
      RETURN
      END 
